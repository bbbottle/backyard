{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/articles",
      "dest": "/api/list_yuque_docs.js"
    },
    {
      "src": "/res",
      "dest": "/api/list_oss_images.js"
    },

    {
      "src": "/blog/articles",
      "dest": "/api/articles.js"
    },
    {
      "src": "/blog/article",
      "dest": "/api/article.js"
    },
    {
      "src": "/gallery/photos",
      "dest": "/api/photos.js"
    }
  ]
}
