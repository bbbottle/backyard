{
  "version": 2,
  "name": "api-grocery",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/z/posts",
      "dest": "/api/z-blog/index.js"
    },
    {
      "src": "/z/voice",
      "dest": "/api/z-blog/voice.js"
    },
    {
      "src": "/auth/editor",
      "dest": "/api/yuque_auth.js"
    },
    {
      "src": "/res",
      "dest": "/api/z-blog/list_oss_images.js"
    },
    {
      "src": "/audios",
      "dest": "/api/z-blog/list_oss_audios.js"
    },
    {
      "src": "/upload",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/upload.js"
    },
    {
      "src": "/z/post/update",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/z-blog/update_post.js"
    },
    {
      "src": "/z/post/delete",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/z-blog/delete_post.js"
    },
    {
      "src": "/z/post/create",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/z-blog/create_post.js"
    }
  ]
}
